/* 附近门店 */
.stores {
	width: 690rpx;
	margin: 0 auto;
	background: #FFFFFF;
	border-radius: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 35rpx 0;
	margin-top: 20rpx;
}
.stores:last-child {
	margin-bottom: 20rpx;
}
.first {
	width: 610rpx;
	margin: 0 auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
