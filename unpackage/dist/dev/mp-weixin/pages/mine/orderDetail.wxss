
template {
	height: 100%;
	background: #f7f8fa;
}
page {
	height: 100%;
	background: #f7f8fa;
}
.blank {
	height: 20rpx;
}

/* 店铺信息 */
.store {
	width: 624rpx;
	margin: 0 auto;
	background: #FFFFFF;
	padding: 0 33rpx;
	border-radius: 20rpx;
	padding-bottom: 50rpx;
	margin-bottom: 20rpx;
}
.storeTop {
	height: 108rpx;
	border-bottom: 1rpx dashed #ebebee;
	font-family: "PingFang SC";
	font-size: 30rpx;
	margin-bottom: 40rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.storeTop image {
	width: 25rpx;
	height: auto;
	display: block;
	margin-right: 19rpx;
}
.storeName {
	font-family: "PingFang SC";
	font-size: 28rpx;
	color: #33344A;
	margin-bottom: 24rpx;
}
.storeAddress {
	font-family: "PingFang SC";
	font-size: 26rpx;
	color: #999AAA;
}

/* 商品信息 */
.goodsInfo {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-bottom: 40rpx;
	border-bottom: 1rpx dashed #ebebee;
}
.goodsInfo image {
	width: 180rpx;
	height: auto;
	display: block;
}
.info {
	width: 100%;
	font-family: "PingFang SC";
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	margin-left: 20rpx;
}
.goodsName {
	font-size: 32rpx;
	color: #33344A;
	margin-bottom: 5rpx;
}
.goodsEname {
	font-size: 20rpx;
	color: #999AAA;
	margin-bottom: 5rpx;
}
.goodsSize {
	font-size: 20rpx;
	color: #999AAA;
	margin-bottom: 5rpx;
}
.priceNum {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.all {
	font-weight: bold;
	font-size: 28rpx;
	color: #33344A;
}
.num {
	font-size: 24rpx;
	color: #33344A;
}
.preferential {
	margin-top: 20rpx;
	font-family: "PingFang SC";
	font-size: 26rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.preferentialLeft {
	color: #999AAA;
}
.preferentialRight {
	color: #33344A;
}
.price {
	color: #EB395B;
}

/* 订单详情 */
.orderInfo {
	margin-top: 20rpx;
	font-family: "PingFang SC";
	font-size: 26rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.infoLeft {
	width: 157rpx;
	display: block;
	color: #999AAA;
}
.infoRight {
	color: #33344A;
}
.footer {
	width: 100%;
	height: 112rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background: #FFFFFF;
	position: fixed;
	bottom: 0;
	left: 0;
}
.footermain {
	width: 690rpx;
	margin: 0 auto;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: reverse;
	-webkit-flex-flow: row-reverse;
	        flex-flow: row-reverse;
}
.button {
	width: 200rpx;
	height: 72rpx;
	background: #EB395B;
	border-radius: 36rpx;
	font-family: "PingFang SC";
	font-size: 28rpx;
	color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
